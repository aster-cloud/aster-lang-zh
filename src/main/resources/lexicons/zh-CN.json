  "punctuation": {
    "statementEnd": "。",
    "listSeparator": "，",
    "enumSeparator": "、",
    "blockStart": "：",
    "stringQuoteOpen": "「",
    "stringQuoteClose": "」"
  },
  "canonicalization": {
    "fullWidthToHalf": true,
    "whitespaceMode": "CHINESE",
    "removeArticles": false,
    "allowedDuplicates": [
      ["BE", "WHEN"],
      ["TYPE_HAS", "TYPE_WITH"],
      ["LESS_THAN", "UNDER"],
      ["GREATER_THAN", "MORE_THAN", "OVER"]
    ],
    "compoundPatterns": [
      {
        "name": "match-when",
        "opener": "MATCH",
        "contextualKeywords": ["WHEN"],
        "closer": "DEDENT"
      },
      {
        "name": "let-be",
        "opener": "LET",
        "contextualKeywords": ["BE"],
        "closer": "NEWLINE"
      }
    ],
    "preTranslationTransformers": [
      "english-possessive",
      "chinese-punctuation",
      "chinese-possessive",
      "chinese-operator",
      "chinese-function-syntax"
    ],
    "postTranslationTransformers": [
      "result-is",
      "set-to",
      "chinese-set-to",
      "chinese-result-is"
    ]
  },
  "messages": {
    "unexpectedToken": "意外的符号：{token}",
    "expectedKeyword": "期望关键词：{keyword}",
    "undefinedVariable": "未定义的变量：{name}",
    "typeMismatch": "类型不匹配：期望 {expected}，实际 {actual}",
    "unterminatedString": "未终止的字符串字面量",
    "invalidIndentation": "无效的缩进：必须是 2 个空格的倍数"
  }
}
